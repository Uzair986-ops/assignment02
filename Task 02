#include<iostream>
using namespace std;
int sumofArray(int arr[], int size);
int main() {
	const int size = 5;
	int num[size] = { 0 };

	for (int i = 0; i < size; i++)
	{
		cout << "Enter Number :";
		cin >> num[i];
	}
	int sum = sumofArray(num, size);
	cout << "Sum of Number is :" << sum << endl;

	return 0;
}
int sumofArray(int arr[], int size) {
	int sum = 0;
	for (int i = 0; i < size; i++)
	{
		sum += arr[i];
	}
	return sum;
}
